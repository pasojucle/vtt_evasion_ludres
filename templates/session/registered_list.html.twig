
    <div>
        <h2>Participants</h2>
        {% if sessions is not empty %}
            <ul>
                {% for session in sessions %}
                    <li class="row row-list">
                        <div class="col-md-12">{{ session.user.member.fullName }}</div>
                    </li>
                {% endfor %}
            </ul>
        {% else %}
            <p>Aucun participant</p>
        {% endif %}
    </div>

