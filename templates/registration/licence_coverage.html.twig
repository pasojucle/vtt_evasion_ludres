{% set licences = form.children.licences %}
{% set boutons_radio_type = (licences[0].children.type is defined) ? form_widget(licences[0].children.type) : '' %}
{% set boutons_radio_coverage = (licences[0].children.coverage is defined) ? form_widget(licences[0].children.coverage) : '' %}
{% set replaces =
    {
        '{{ boutons_radio_type }}': boutons_radio_type,
        '{{ boutons_radio_assurance }}': boutons_radio_coverage,
    }
%}

<div class="container">{{ current.content|replace(replaces) | raw}}</div>

<br>



        

