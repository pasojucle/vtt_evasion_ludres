<ul class="block background-ligth">
    <li class="row">
        <span class="col-md-4 col-sx-6"><b>Type d'assurance : </b></span><span class="col-md-8 col-sx-6">{{ progress.user.lastLicence.coverageStr|raw}}</span>
        <span class="col-md-4 col-sx-6"><b>Options d'assurance : </b></span>
        <span class="col-md-8 col-sx-6">
            {% for option in progress.user.lastLicence.options %}
                {% set optionEnum = enum('App\\Entity\\Enum\\LicenceOptionEnum').from(option) %}
                {% if not loop.first %} - {% endif %}
                {{ optionEnum|trans }}
            {% endfor %}
        </span>
        <span class="col-md-4 col-sx-6"></span><span class="col-md-8 col-sx-6">{{ progress.user.lastLicence.isVae|raw }}</span>
    </li>
</ul>